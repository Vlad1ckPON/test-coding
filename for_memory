def main(self, user, bet):
        enemy = random.choice(self.SIGNS)
        if self.coins < bet:
            return 'Нет коинов', self.coins, enemy
        self.rebalance( user, enemy, bet)
        return self.check(user, enemy), self.coins, enemy, user


